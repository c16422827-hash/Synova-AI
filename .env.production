# Production Environment Variables for Synova AI FastAPI

# Application
ENVIRONMENT=production
DEBUG=False
SECRET_KEY=your-secret-key-change-in-production

# Database
DATABASE_URL=postgresql://postgres:postgres@db:5432/synova_dev
